{
    "compilerOptions": {
      "target": "ES5",
      "module": "ESNext",
      "jsx": "react-jsx",
      "strict": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "lib": [
        "DOM",
        "DOM.Iterable",
        "ESNext"
      ],
      "allowJs": true,
      "skipLibCheck": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noUnusedLocals": true,
      "noEmit": false,
      "jsxImportSource": "@emotion/react"
    },
    "include": [
      "src/**/**/*",
  
    ],
    "exclude": [
      "node_modules",
    ],
}